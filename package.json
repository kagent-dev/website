{
  "name": "kagent-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "generate-nav": "node ./scripts/generate-nav.mjs",
    "check-links": "node ./scripts/check-links.mjs",
    "generate-sitemap": "node ./scripts/sitemap.js",
    "dev": "npm run generate-nav && npm run check-links && npm run generate-sitemap && next dev",
    "build": "npm run generate-nav && npm run check-links && npm run generate-sitemap && next build",
    "start": "next start",
    "lint": "next lint",
    "build:worker": "npm run generate-nav && npm run check-links && npm run generate-sitemap && npx opennextjs-cloudflare build",
    "dev:worker": "wrangler dev --port 3000",
    "preview:worker": "npm run build:worker && npm run dev:worker",
    "deploy:worker": "npm run build:worker && wrangler deploy --minify",
    "dry-run:worker": "wrangler deploy --minify --outdir bundler/ --dry-run"
  },
  "dependencies": {
    "@docsearch/react": "^3.9.0",
    "@heroicons/react": "^2.2.0",
    "@mdx-js/loader": "^3.1.1",
    "@mdx-js/react": "^3.1.1",
    "@next/mdx": "^15.5.3",
    "@next/third-parties": "^15.5.3",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/typography": "^0.5.19",
    "@types/mdx": "^2.0.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "framer-motion": "^12.23.13",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.543.0",
    "next": "^15.5.2",
    "next-mdx-remote": "^5.0.0",
    "next-themes": "^0.4.6",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-markdown": "^10.1.0",
    "rehype-unwrap-images": "^1.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@opennextjs/cloudflare": "^1.6.5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "wrangler": "^4.32.0",
    "yaml-loader": "^0.8.1"
  }
}
